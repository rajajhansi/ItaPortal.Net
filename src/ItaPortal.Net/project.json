{
	"webroot": "wwwroot",
	"version": "1.0.0-*",
	"dependencies": {
		"Microsoft.Framework.ConfigurationModel": "1.0.0-beta3",
		"Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta3",
		"Microsoft.Framework.Logging": "1.0.0-beta3",
		"Microsoft.Framework.Logging.Console": "1.0.0-beta3",
		"Microsoft.AspNet.Server.IIS": "1.0.0-beta3",
		"Microsoft.AspNet.Server.WebListener": "1.0.0-beta3",
		"Microsoft.AspNet.Diagnostics": "1.0.0-beta3",
		"Microsoft.AspNet.Diagnostics.Entity": "7.0.0-beta3",
		"Microsoft.AspNet.StaticFiles": "1.0.0-beta3",
		"Microsoft.AspNet.Identity.EntityFramework": "3.0.0-beta3",
		"Microsoft.AspNet.Mvc": "6.0.0-beta3",	
		"Microsoft.AspNet.WebUtilities": "1.0.0-beta3",	
		"Microsoft.AspNet.Security": "1.0.0-beta3",
		"Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0-beta3",
		"EntityFramework.SqlServer": "7.0.0-beta3",
		"EntityFramework.Commands": "7.0.0-beta3",
		"Sendgrid": "5.1.0"
	},
	 "commands": {
        /* Change the port number when you are self hosting this application */
        "web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:85",
        "gen": "Microsoft.Framework.CodeGeneration",
        "ef":  "EntityFramework.Commands"
    },
	"frameworks": {
		"aspnet50": { }
		//"aspnetcore50": { }
	},
	"bundleExclude": [
		"node_modules",
		"bower_components",
		"**.kproj",
		"**.user",
		"**.vspscc"
	],
	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components"
	]
}
